generator client {
    provider = "prisma-client-js"
}

generator zod {
  provider                 = "zod-prisma"
  output                   = "./zod"
  relationModel            = true
  modelCase                = "PascalCase"
  modelSuffix              = "Model"
  useDecimalJs             = true
  prismaJsonNullability    = true
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {

    id String @id @default(uuid())

}

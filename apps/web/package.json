{
    "name": "@abuse-sleuth/web",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "concurrently -n Stripe,Next \"npm:stripe:listen-webhook\" \"npm:dev:next\"",
        "dev:next": "next dev -p 3000",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
        "build": "next build",
        "start": "next start -p 3000",
        "lint": "next lint",
        "test": "run-s e2e:headless",
        "cypress": "cypress open",
        "cypress:headless": "cypress run",
        "e2e": "start-server-and-test dev http://localhost:3000 cypress",
        "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
        "stripe:listen-webhook": "stripe listen --forward-to localhost:3000/api/stripe/webhook"
    },
    "dependencies": {
        "@abuse-sleuth/auth": "*",
        "@abuse-sleuth/prisma": "*",
        "@abuse-sleuth/ui": "*",
        "@fortawesome/fontawesome-svg-core": "^1.3.0",
        "@fortawesome/free-brands-svg-icons": "^6.0.0",
        "@fortawesome/free-solid-svg-icons": "^6.0.0",
        "@fortawesome/react-fontawesome": "^0.1.17",
        "@mantine/dropzone": "^3.6.14",
        "@mantine/form": "^4.0.1",
        "@mantine/hooks": "^4.0.1",
        "@mantine/next": "^3.6.11",
        "@sentry/nextjs": "^6.19.2",
        "@sentry/node": "^6.19.2",
        "@stripe/stripe-js": "^1.24.0",
        "axios": "^0.26.0",
        "bcryptjs": "^2.4.3",
        "cookies-next": "^2.0.4",
        "dayjs": "^1.10.8",
        "geoip-lite": "^1.4.3",
        "ip": "^1.1.5",
        "ip-regex": "^5.0.0",
        "joi": "^17.6.0",
        "micro": "^9.3.4",
        "next": "^12.1.0",
        "next-connect": "^0.12.2",
        "next-transpile-modules": "9.0.0",
        "papaparse": "^5.3.1",
        "preact": "^10.7.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-ssr-prepass": "^1.5.0",
        "stripe": "^8.205.0",
        "swr": "^1.2.2"
    },
    "devDependencies": {
        "@abuse-sleuth/config": "*",
        "@abuse-sleuth/tsconfig": "*",
        "@types/ip": "^1.1.0",
        "@types/node": "^17.0.12",
        "@types/react": "17.0.37",
        "concurrently": "^7.0.0",
        "cross-env": "^7.0.3",
        "cypress": "^9.5.3",
        "dotenv": "^16.0.1",
        "eslint": "7.32.0",
        "start-server-and-test": "^1.14.0",
        "typescript": "^4.5.3"
    }
}

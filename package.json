{
    "name": "Abuse-Sleuth",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*",
        "infra"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^3.2.0",
        "eslint-config-custom": "latest",

{
    "name": "sentinel",
    "private": true,
    "scripts": {
        "dev": "npm run dev:build-functions && concurrently -c cyan,magenta,#ffa500 -n NextJS,Stripe,Firebase \"npm:dev:next\" \"npm:dev:stripe\" \"npm:dev:emulators\"",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "dev:build-functions": "npm run build --prefix functions",
        "dev:next": "next dev",
        "dev:stripe": "stripe listen --forward-to localhost:3000/api/stripe/webhook",
        "dev:emulators": "firebase emulators:start --export-on-exit=./emulator-export --import=./emulator-export",
        "deploy": "firebase deploy --only functions,firestore"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.3.0",
        "@fortawesome/free-brands-svg-icons": "^6.0.0",
        "@fortawesome/free-solid-svg-icons": "^6.0.0",
        "@fortawesome/react-fontawesome": "^0.1.17",
        "@mantine/core": "^3.5.3",
        "@mantine/dropzone": "^3.6.2",
        "@mantine/hooks": "^3.5.3",
        "@mantine/next": "^3.5.3",
        "@sentry/nextjs": "^6.17.7",
        "@stripe/stripe-js": "^1.22.0",
        "axios": "^0.24.0",
        "chalk": "^5.0.0",
        "dayjs": "^1.10.7",
        "firebase": "^9.6.6",
        "firebase-admin": "^10.0.2",
        "joi": "^17.5.0",
        "micro": "^9.3.4",
        "next": "12.0.10",
        "next-joi": "^2.2.1",
        "nookies": "^2.5.2",
        "papaparse": "^5.3.1",
        "preact": "^10.6.4",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "safe-json-stringify": "^1.2.0",
        "stripe": "^8.201.0",
        "swr": "^1.2.1"
    },
    "devDependencies": {
        "@faker-js/faker": "^6.0.0-alpha.5",
        "@types/micro": "^7.3.6",
        "@types/node": "17.0.8",
        "@types/papaparse": "^5.3.1",
        "@types/react": "17.0.38",
        "@types/winston": "^2.4.4",
        "babel-plugin-superjson-next": "^0.4.2",
        "concurrently": "^7.0.0",
        "eslint": "8.6.0",
        "eslint-config-next": "12.0.7",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "prettier": "^2.5.1",
        "typescript": "4.5.4"
    }
}
